{
  "rewrites": [
    {
      "source": "/uploads/:path",
      "destination": "https://optimage-uploads.s3.amazonaws.com/:path"
    }
  ],
  "headers": [
    {
      "source": "/uploads/:path",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, s-maxage=86400, max-age=86400, immutable"
        }
      ]
    }
  ]
}
